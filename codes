package basic_programs;
public class Sample_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=4;
		float b=(float) 3.1;
		double c=99.9;
		char d='c';
		String e="qwerty";
		System.out.println("a is int and its size is "+Integer.BYTES);
		System.out.println("b is float and its size is "+Float.BYTES);
		System.out.println("c is double and its size is "+Double.BYTES);
		System.out.println("d is char and its size is "+Character.BYTES);
		System.out.println("e is string and its size is "+Character.BYTES);
		
	}

}
